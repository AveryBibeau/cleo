{
  "name": "jsx-ssr",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "scripts": {
    "start-old": "NODE_ENV=production node --enable-source-maps ./dist/index.js",
    "start": "NODE_ENV=production node --enable-source-maps ./server.js",
    "dev": "NODE_ENV=development node ./server.js",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "VITE_BUILD=client vite build --outDir dist/client",
    "build:server": "VITE_BUILD=ssr vite build --outDir dist/server --ssr src/app.tsx",
    "dev-old": "pnpm clean && pnpm copy-files && NODE_ENV=development tsc-watch --onSuccess \"node --enable-source-maps ./dist/dev.js\"",
    "build-old": "pnpm clean && pnpm copy-files && tsc && node --enable-source-maps ./dist/build.js",
    "clean": "rm -rf ./dist",
    "copy-files": "mkdir -p ./dist/public && cp -R ./src/public/ ./dist/public/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^6.0.0",
    "@fastify/sensible": "^4.0.0",
    "@fastify/session": "^8.2.0",
    "@fastify/static": "^5.0.1",
    "dotenv": "^16.0.0",
    "fastify": "^3.0.0",
    "middie": "^6.0.0",
    "path-to-regexp": "^6.2.0",
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.1.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.1.0",
    "esbuild": "^0.14.38",
    "esbuild-plugin-manifest": "^0.5.0",
    "esbuild-sass-plugin": "^2.2.6",
    "pino-pretty": "^7.6.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3",
    "vite": "^2.9.7",
    "vite-plugin-checker": "^0.4.6"
  }
}
