{
  "name": "jsx-ssr",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node --enable-source-maps ./server.js",
    "dev": "nodemon --watch ./server.js --watch ./src/app.ts --watch ./src/router.ts --ignore ./dist/ --ext js,ts,jsx,tsx,css,scss,json --exec 'NODE_ENV=development node --enable-source-maps --inspect ./server.js'",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "VITE_BUILD=client vite build --outDir dist/client",
    "build:server": "VITE_BUILD=ssr vite build --outDir dist/server --ssr src/app.ts"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/middie": "^8.0.0",
    "@fastify/static": "^6.5.0",
    "@fastify/type-provider-typebox": "^2.3.0",
    "@sinclair/typebox": "^0.24.42",
    "fastify": "^4.6.0",
    "lodash-es": "^4.17.21",
    "path-to-regexp": "^6.2.1",
    "preact": "^10.11.0",
    "preact-render-to-string": "^5.2.4",
    "sade": "^1.8.1",
    "typescript": "^4.8.3",
    "uuid": "^8.3.2",
    "vite": "^3.1.2",
    "vite-plugin-checker": "^0.5.1"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.45",
    "@types/uuid": "^8.3.4",
    "pino-pretty": "^7.6.1"
  }
}
