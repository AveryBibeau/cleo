{
  "name": "jsx-ssr",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production && node --enable-source-maps ./dist/index.js",
    "dev": "pnpm clean && pnpm copy-files && pnpm copy-env && NODE_ENV=development tsc-watch -p ./tsconfig-dev.json --onSuccess \"node --enable-source-maps --inspect ./dist/dev.js\"",
    "build": "pnpm clean && pnpm copy-files && NODE_ENV=production && tsc -p ./tsconfig-prod.json && node --enable-source-maps ./dist/build.js",
    "clean": "rm -rf ./dist",
    "copy-files": "mkdir -p ./dist/public && cp -R ./src/public/. ./dist/public/.",
    "copy-env": "cp ./.env ./dist/"
  },
  "imports": {
    "##/*": "./dist/*.js"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^3.0.2",
    "@fastify/cookie": "^7.0.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/csrf-protection": "^5.0.0",
    "@fastify/formbody": "^7.0.1",
    "@fastify/helmet": "^9.1.0",
    "@fastify/sensible": "^5.1.0",
    "@fastify/session": "^9.0.0",
    "@fastify/static": "^6.4.0",
    "@fastify/type-provider-typebox": "^2.0.1",
    "@sinclair/typebox": "^0.24.7",
    "dotenv": "^16.0.0",
    "fastify": "^4.2.0",
    "lodash-es": "^4.17.21",
    "path-to-regexp": "^6.2.1",
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.1.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.25",
    "@types/postcss-import": "^14.0.0",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.7",
    "chalk": "^5.0.1",
    "concurrently": "^7.1.0",
    "cssnano": "^5.1.12",
    "esbuild": "^0.14.38",
    "esbuild-plugin-manifest": "^0.5.0",
    "esbuild-sass-plugin": "^2.2.6",
    "esbuild-style-plugin": "^1.6.0",
    "pino-pretty": "^7.6.1",
    "postcss": "^8.4.14",
    "sass": "^1.53.0",
    "tailwindcss": "^3.1.4",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
