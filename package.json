{
  "name": "jsx-ssr",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node --enable-source-maps ./dist/index.js",
    "dev": "pnpm clean && pnpm copy-files && NODE_ENV=development tsc-watch --onSuccess \"node --enable-source-maps ./dist/dev.js\"",
    "build": "pnpm clean && pnpm copy-files && tsc && node --enable-source-maps ./dist/build.js",
    "clean": "rm -rf ./dist",
    "copy-files": "mkdir -p ./dist/public && cp -R ./src/public/ ./dist/public/"
  },
  "imports": {
    "##/*": "./dist/*.js"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.0",
    "fastify": "^3.0.0",
    "fastify-sensible": "^3.1.2",
    "fastify-static": "^4.6.1",
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.1.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.1.0",
    "esbuild": "^0.14.38",
    "esbuild-plugin-manifest": "^0.5.0",
    "esbuild-sass-plugin": "^2.2.6",
    "pino-pretty": "^7.6.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3"
  }
}
