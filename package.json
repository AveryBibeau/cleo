{
  "name": "jsx-ssr",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node --enable-source-maps ./server.js",
    "dev": "nodemon --watch ./src/app.ts --watch ./src/routes --ignore ./dist/ --ext js,ts,jsx,tsx,css,scss,json --exec 'NODE_ENV=development node --enable-source-maps --inspect ./server.js'",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "VITE_BUILD=client vite build --outDir dist/client",
    "build:server": "VITE_BUILD=ssr vite build --outDir dist/server --ssr src/app.ts"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^6.0.0",
    "@fastify/sensible": "^4.0.0",
    "@fastify/session": "^8.2.0",
    "@fastify/static": "^5.0.1",
    "@types/node": "^17.0.25",
    "@types/uuid": "^8.3.4",
    "dotenv": "^16.0.0",
    "fastify": "^3.0.0",
    "middie": "^6.0.0",
    "path-to-regexp": "^6.2.0",
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.1.21",
    "sass": "^1.51.0",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2",
    "vite": "^2.9.13"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "pino-pretty": "^7.6.1",
    "vite-plugin-checker": "^0.4.6"
  }
}
